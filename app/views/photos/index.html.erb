<h1>All photos</h1>

<% @photos.each do |photo| %>
	<div class="page_wrapper">
		<h2 class="title">
			<%= link_to photo.title, photo %>
		</h2>
		<%= image_tag photo.image.url(:medium) %>
	</div>
<% end %>

<%= will_paginate @photos, renderer: BootstrapPagination::Rails, previous_label: "Older", next_label: "Newer" %>